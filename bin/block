#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile" ''

  exec block require "$@"
}

umask 022
if [[ "${PWD:0:${#BLOCK_PATH}}" != "$BLOCK_PATH" ]]; then
  export BLOCK_META=1
fi
if type -P sub >/dev/null; then
  source sub "$0" "$@"
fi
